import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
        int a[]=new int[5];
        Scanner s1=new Scanner(System.in);
        int sum=0;
        int i=0;
        for(i=0;i<a.length;i++)
            a[i]=s1.nextInt();
        for(i=0;i<a.length;i++){
            sum=sum+a[i];
    }
        for(i=0;i<a.length;i++){
            a[i]=sum-a[i];
        }
        for(i=0;i<a.length;i++)
        System.out.println(a[i]);
        
    }
}
